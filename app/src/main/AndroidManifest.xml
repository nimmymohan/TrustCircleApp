<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wsu.csc5991.trustcircle">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActSignIn"></activity>
        <activity android:name=".ActJoinCircle"></activity>
        <activity android:name=".ActMemberSignUp"></activity>
        <activity android:name=".ActCircleSignUp"></activity>
        <activity android:name=".ActEditCircle"></activity>
        <activity android:name=".ActDeleteCircle"></activity>
        <activity android:name=".ActDisplayCircle"></activity>
        <activity android:name=".ActHelp"></activity>
    </application>

</manifest>